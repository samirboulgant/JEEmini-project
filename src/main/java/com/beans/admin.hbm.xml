<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.beans.Admin" table="brikol.administrateur">
		<id column="CodeAdmin" name="code_admin" type="java.lang.Integer" />
		<property column="Email" name="email" type="java.lang.String" />
		<property column="MotDePasse" name="password" type="string" />
	</class>
	
</hibernate-mapping>

